<html>
    <head>
        <meta charset='utf-8'>
        
        <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
        
        <link rel="stylesheet" href="https://bootswatch.com/cosmo/bootstrap.min.css">
        <link rel="stylesheet" href="https://unpkg.com/purecss@1.0.0/build/pure-min.css" integrity="sha384-nn4HPE8lTHyVtfCBi5yW9d20FjT8BJwUXyWZT9InLYax14RDjBj46LmSztkmNP9w" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css?family=Acme" rel="stylesheet">
        <link rel="stylesheet" href="~/css/styles.css">

        <title>Cojovey</title>
    </head>
    <body>
        <div class="container">
            <div class="jumbotron">
                <h1>Coding Dojo Survey</h1>
            </div>
            <div class="row">
                <div class="col-xs-12">
                    <h1><span class="bold">Name: </span> @ViewBag.Name</h1>
                    <h1><span class="bold">Dojo: </span> @ViewBag.Dojo</h1>
                    <h1><span class="bold">Favorite Language: </span> @ViewBag.Language</h1>
                    <h1><span class="bold">Comments: </span> @ViewBag.Comments</h1>
                </div>
            </div>
            <button><a href="/">HOME</a></button>
        </div>
    </body>
</html>